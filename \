#include "libft/libft.h"
#include "minishell.h"

int	echoval(char *info)
{
	int		i;
	char	*s;
	int		n;

	i = -1;
	ii = -1;
	n = 0;
	if (info[0] == '-')
	{
		while (info[++i])
		{
			if (info[i][ii] == 'n')
				{
					n = 1;
				}
				else
				{
				n = 0;
				break ;
			}
		}
	}
	return (n);
}

void	echo(char **info)
{
	int		n;

	n = echoval(info);

}
